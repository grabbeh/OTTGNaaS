{
  "name": "ottgnaas",
  "version": "0.0.1",
  "description": "Online terms as a graphic novel",
  "main": "index.js",
  "dependencies": {
    "@google-cloud/vision": "^0.11.3",
    "browser-request": "^0.3.3",
    "canvas": "^1.6.5",
    "domready": "^1.0.8",
    "express": "^4.15.3",
    "fabric": "^1.7.12",
    "google-images": "^2.1.0",
    "jimp": "^0.2.28",
    "lodash": "^4.17.4",
    "node-kmeans": "^1.1.0",
    "node-tesseract": "^0.2.7",
    "quick-hull-2d": "0.1.0",
    "superagent": "^3.5.2",
    "tesseract.js": "^1.0.10",
    "underscore": "1.8.3",
    "unfluff": "^1.1.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "browserify": "^14.4.0",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0"
  },
  "scripts": {
    "server": "babel server -watch --out-dir compiled",
    "watch": "watchify public/js/main.js -o public/dist/bundle.js",
    "build-previous": "browserify public/js/main.js -o public/dist/bundle.js",
    "build": "browserify -t [ babelify --presets [ react ] ] client/index.js -o public/dist/react-bundle.js",
    "build-full": "npm run build-previous && npm run build"
  },
  "babel": {
    "presets": [
      "node6",
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grabbeh/OTTGNaaS.git"
  },
  "author": "grabbeh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grabbeh/OTTGNaaS/issues"
  },
  "homepage": "https://github.com/grabbeh/OTTGNaaS#readme"
}